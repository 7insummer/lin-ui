.calendar-day-container{display:flex;flex-direction:column;height:100%;align-items:center}.calendar-day-container.selected{background-color:#58bd6b;color:#fff}.calendar-day-container.start{background-color:#58bd6b;color:#fff}.calendar-day-container.end{background-color:#58bd6b;color:#fff}.calendar-day-container.disabled{color:#c8c9cc;cursor:default;background:#fff}.calendar-day-container.middle{background-color:#eff9f0;color:#58bd6b}.calendar-day-container .top{height:44rpx;line-height:44rpx;font-size:24rpx}.calendar-day-container .text{flex:1}.calendar-day-container .bottom{height:44rpx;line-height:44rpx;font-size:24rpx}